!
version 12.4
!
hostname d
!
interface GigabitEthernet0/1
    description b:Gi0/2
    ip address 100.0.2.4 255.255.255.0
    ip ospf cost 1
!
interface GigabitEthernet0/2
    description c:Gi0/2
    ip address 100.0.1.4 255.255.255.0
    ip ospf cost 1
!
interface GigabitEthernet0/3
    description f:Gi0/1
    ip address 102.0.1.4 255.255.255.0
    ip ospf cost 20
!
interface GigabitEthernet0/4
    description g:Gi0/2
    ip address 103.0.0.4 255.255.255.0
    ip ospf cost 10
!
router ospf 1
    network 100.0.0.0 0.0.255.255 area 0
    area 102 stub
    network 102.0.0.0 0.0.255.255 area 102
    area 103 stub
    network 103.0.0.0 0.0.255.255 area 103
!
end
