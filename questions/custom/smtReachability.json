{
    "class": "org.batfish.minesweeper.question.SmtReachabilityQuestionPlugin$ReachabilityQuestion",
    "differential": false,
    "ingressNodeRegex": "${ingressNodeRegex}",
    "finalNodeRegex": "${finalNodeRegex}",
    "finalIfaceRegex": "${finalIfaceRegex}",
    "srcIps": "${srcIps}",
    "dstIps": "${dstIps}",
    "failures": "${failures}",
    "failNode1Regex": "${failNode1Regex}",
    "failNode2Regex": "${failNode2Regex}",
    "notFailNode1Regex": "${notFailNode1Regex}",
    "notFailNode2Regex": "${notFailNode2Regex}",
    "diffType": null,
    "envDiff": false,
    "benchmark": true,
    "fullModel": true,
    "baseEnvType": "none",
    "deltaEnvType": "none",
    "negate": "${negate}",
    "ipProtocols": "${protocols}",
    "instance": {
        "description": "Ensure that the network is tolerant to interface failures",
        "instanceName": "smtReachability",
        "tags": [
            "dataPlane",
            "reachability",
            "experimental"
        ],
        "variables": {
            "ingressNodeRegex": {
                "description": "Starting nodes",
                "optional": true,
                "type": "nodeSpec",
                "value": ".*"
            },
            "finalNodeRegex": {
                "description": "Final nodes",
                "type": "nodeSpec",
                "optional": true,
                "value": ".*"
            },
            "finalIfaceRegex": {
                "description": "Final interfaces",
                "type": "string",
                "optional": true,
                "value": ".*"
            },
            "srcIps": {
                "description": "srcIPs",
                "type": "string",
                "minElements": 0,
                "optional": true,
                "value": 1
            },
            "dstIps": {
                "description": "dstIPs",
                "type": "string",
                "minElements": 0,
                "optional": true,
                "value": 1
            },
            "protocols": {
                "description": "protocols",
                "type": "string",
                "minElements": 0,
                "optional": true,
                "value": 1
            },
            "failures": {
                "description": "Number of failures to analyze",
                "type": "integer",
                "optional": true,
                "value": 1
            },
            "failNode1Regex": {
                "description": "Regex to specify the first node that is part of the link to fail",
                "type": "nodeSpec",
                "optional": true,
                "value": ".*"
            },
            "failNode2Regex": {
                "description": "Regex to specify the second node that is part of the link to fail",
                "type": "nodeSpec",
                "optional": true,
                "value": ".*"
            },
            "notFailNode1Regex": {
                "description": "Regex to specify the first node that cannot be part of the link to fail",
                "type": "nodeSpec",
                "optional": true,
                "value": ""
            },
            "notFailNode2Regex": {
                "description": "Regex to specify the second node that cannot be part of the link to fail",
                "type": "nodeSpec",
                "optional": true,
                "value": ""
            },
            "negate": {
                "description": "Check for absence of reachability",
                "type": "boolean",
                "optional": true,
                "value": false
            }
        }
    }
}
