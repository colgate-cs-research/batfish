{
    "class": "org.batfish.minesweeper.question.SmtPathPreferenceQuestionPlugin$PathPreferenceQuestion",
    "differential": false,
    "ingressNodeRegex": "${ingressNodeRegex}",
    "finalNodeRegex": "${finalNodeRegex}",
    "finalIfaceRegex": "${finalIfaceRegex}",
    "srcIps": "${srcIps}",
    "dstIps": "${dstIps}",
    "failures": "${failures}",
    "failNode1Regex": "${failNode1Regex}",
    "failNode2Regex": "${failNode2Regex}",
    "prefs": "${prefs}",
    "diffType": null,
    "envDiff": false,
    "benchmark": true,
    "fullModel": true,
    "baseEnvType": "none",
    "deltaEnvType": "none",
    "instance": {
        "description": "Check path preferences",
        "instanceName": "smtPathPreference",
        "tags": [
            "dataPlane",
            "reachability",
            "experimental"
        ],
        "variables": {
            "ingressNodeRegex": {
                "description": "Starting nodes",
                "optional": true,
                "type": "nodeSpec",
                "value": ".*"
            },
            "finalNodeRegex": {
                "description": "Final nodes",
                "type": "nodeSpec",
                "optional": true,
                "value": ".*"
            },
            "finalIfaceRegex": {
                "description": "Final interfaces",
                "type": "string",
                "optional": true,
                "value": ".*"
            },
            "srcIps": {
                "description": "srcIPs",
                "type": "string",
                "minElements": 0,
                "optional": true,
                "value": 1
            },
            "dstIps": {
                "description": "dstIPs",
                "type": "string",
                "minElements": 0,
                "optional": true,
                "value": 1
            },
            "failures": {
                "description": "Number of failures to analyze",
                "type": "integer",
                "optional": true,
                "value": 1
            },
            "failNode1Regex": {
                "description": "Regex to specify the first node that is part of the link to fail",
                "type": "nodeSpec",
                "optional": true,
                "value": ".*"
            },
            "failNode2Regex": {
                "description": "Regex to specify the second node that is part of the link to fail",
                "type": "nodeSpec",
                "optional": true,
                "value": ".*"
            },
            "prefs": {
                "description": "prefs",
                "type": "string",
                "minElements": 1,
                "optional": false
            }
        }
    }
}
